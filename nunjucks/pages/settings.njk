            $( ".js-increment-down" ).on( "click", function() {
                var input = $(this).parents('.form-group').find('input');
                var currentVal = $(input).val();
                var units = $(input).data('units');
                var max = Number($(input).data('max'));
                var min = Number($(input).data('min'));
                var increment = Number($(input).data('increment'));
                // https://stackoverflow.com/questions/36321409/javascript-remove-all-charater-except-leading-one-dot-and-digits
                var number = Number(currentVal.replace(/[^0-9.-]/g, '').replace(/(\..*)\./g, '$1').replace(/(?!^)-/g, ''));
                var finalNumb = number;
                if (number > min && number <= max) {
                    // https://stackoverflow.com/questions/10473994/javascript-adding-decimal-numbers-issue
                    finalNumb = Math.round((number - increment) * 1e12) / 1e12;
                } else if (number <= min) {
                    finalNumb = min;
                }
                if (finalNumb < min) {
                    finalNumb = min;
                }
                $(input).val(finalNumb + units);
            });

            $( ".js-increment-up" ).on( "click", function() {
                var input = $(this).parents('.form-group').find('input');
                var currentVal = $(input).val();
                var units = $(input).data('units');
                var max = Number($(input).data('max'));
                var min = Number($(input).data('min'));
                var increment = Number($(input).data('increment'));
                // https://stackoverflow.com/questions/36321409/javascript-remove-all-charater-except-leading-one-dot-and-digits
                var number = Number(currentVal.replace(/[^0-9.-]/g, '').replace(/(\..*)\./g, '$1').replace(/(?!^)-/g, ''));
                var finalNumb = number;
                if (number >= min && number < max) {
                    // https://stackoverflow.com/questions/10473994/javascript-adding-decimal-numbers-issue
                    finalNumb = Math.round((number + increment) * 1e12) / 1e12;
                } else if (number >= max) {
                    finalNumb = max;
                }
                if (finalNumb > max) {
                    finalNumb = max;
                }
                $(input).val(finalNumb + units);
            });

