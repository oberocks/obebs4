{%- macro alert_colors(classPartial, namePartial) %}
<div class="js-prism">
    <div class="alert alert-{{ classPartial }} alert-dismissible mb-2 fade show text-left" role="alert">
        <h4 class="alert-heading">{{ namePartial }} Alert</h4>
        <hr />
        <p>This is an example alert component with <a href="#" class="alert-link">an example link</a>! You can use this to display important information to a user in a way that visually stands out.</p>
        <hr />
        <p class="mb-0">Note, you can use margin utilities to keep things tidy.</p>
        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    </div>
</div>
{%- endmacro %}



