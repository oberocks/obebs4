    <div class="col-7">
        <div class="form-group{% if note != 'false' %} mb-0{% endif %}">
            <label class="d-block font-weight-bold">{{ label }} Color</label>
            <select id="{{ id }}" class="form-control"{% if note != 'false' %} aria-describedby="{{id}}-help"{% endif %}>
                {% if selection1 === 'none' %}<option value="none" selected>None</option>{% endif %}
                <option value="white"{% if selection1 == 'white' %} selected{% endif %}>White</option>
                <option value="light-gray"{% if selection1 == 'light-gray' %} selected{% endif %}>Light Gray</option>
                <option value="gray"{% if selection1 == 'gray' %} selected{% endif %}>Gray</option>
                <option value="dark-gray"{% if selection1 == 'dark-gray' %} selected{% endif %}>Dark Gray</option>
                <option value="black"{% if selection1 == 'black' %} selected{% endif %}>Black</option>
                <option value="red"{% if selection1 == 'red' %} selected{% endif %}>Red</option>
                <option value="rose"{% if selection1 == 'rose' %} selected{% endif %}>Rose</option>
                <option value="magenta"{% if selection1 == 'magenta' %} selected{% endif %}>Magenta</option>
                <option value="violet"{% if selection1 == 'violet' %} selected{% endif %}>Violet</option>
                <option value="blue"{% if selection1 == 'blue' %} selected{% endif %}>Blue</option>
                <option value="azure"{% if selection1 == 'azure' %} selected{% endif %}>Azure</option>
                <option value="cyan"{% if selection1 == 'cyan' %} selected{% endif %}>Cyan</option>
                <option value="spring-green"{% if selection1 == 'spring-green' %} selected{% endif %}>Spring Green</option>
                <option value="green"{% if selection1 == 'green' %} selected{% endif %}>Green</option>
                <option value="chartreuse"{% if selection1 == 'chartreuse' %} selected{% endif %}>Chartreuse</option>
                <option value="yellow"{% if selection1 == 'yellow' %} selected{% endif %}>Yellow</option>
                <option value="orange"{% if selection1 == 'orange' %} selected{% endif %}>Orange</option>
                <option value="orange-gray"{% if selection1 == 'orange-gray' %} selected{% endif %}>Orange Gray</option>
                <option value="dark-gray-orange"{% if selection1 == 'dark-gray-orange' %} selected{% endif %}>Dark Gray Orange</option>
                <option value="blue-gray"{% if selection1 == 'blue-gray' %} selected{% endif %}>Blue Gray</option>
                <option value="light-gray-azure"{% if selection1 == 'light-gray-azure' %} selected{% endif %}>Light Gray Azure</option>
            </select>
        </div>
    </div>
    <div class="col-5">
        <div class="form-group{% if note != 'false' %} mb-0{% endif %}">
            <label class="d-block font-weight-bold">Tint / Shade</label>
            <select id="{{ id }}-modifier" class="form-control">
                <option value="50"{% if selection2 == '50' %} selected{% endif %}>50</option>
                <option value="100"{% if selection2 == '100' %} selected{% endif %}>100</option>
                <option value="200"{% if selection2 == '200' %} selected{% endif %}>200</option>
                <option value="300"{% if selection2 == '300' %} selected{% endif %}>300</option>
                <option value="400"{% if selection2 == '400' %} selected{% endif %}>400</option>
                <option value="none"{% if selection2 == 'none' %} selected{% endif %}>None</option>
                <option value="600"{% if selection2 == '600' %} selected{% endif %}>600</option>
                <option value="700"{% if selection2 == '700' %} selected{% endif %}>700</option>
                <option value="800"{% if selection2 == '800' %} selected{% endif %}>800</option>
                <option value="900"{% if selection2 == '900' %} selected{% endif %}>900</option>
            </select>
        </div>
    </div>
</div>
{% if note != 'false' %}<small id="{{id}}-help" class="form-text pb-2" style="opacity: 0.6;">{{ note }}</small>{% endif %}
