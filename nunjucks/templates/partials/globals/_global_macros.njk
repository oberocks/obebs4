    <a class="nav-link{% if active === 'true' %} active{% endif %}" href="#">{% if active === 'true' %}Active{% else %}Link 1{% endif %}</a>
</li>
<li class="nav-item">
    <a class="nav-link" href="#">{% if active === 'true' %}Super-Long Link{% else %}Link 2{% endif %}</a>
</li>
<li class="nav-item">
    <a class="nav-link disabled" href="#">Disabled</a>
</li>
{%- endmacro %}



{%- macro navbar_colors(classPartial, namePartial, darkLightPartial, inputPartial, btnOutlinePartial) %}
<div class="js-prism text-left mb-2">
    <nav class="navbar navbar-expand-lg navbar-{{ darkLightPartial }} bg-{{ classPartial }}">

        <a class="navbar-brand" href="#">{{ namePartial }}</a>

        <button class="navbar-toggler" type="button" data-toggle="collapse" aria-expanded="false" aria-label="Toggle navigation" data-target="#navbar-{{ classPartial }}" aria-controls="navbar-{{ classPartial }}">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbar-{{ classPartial }}">

            <ul class="navbar-nav mr-auto">
                <li class="nav-item active">
                    <a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">Link</a>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbar-dropdown-menu-{{ classPartial }}" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Dropdown
                    </a>
                    <div class="dropdown-menu dropdown-menu-{{ classPartial }}" aria-labelledby="navbar-dropdown-menu-{{ classPartial }}">
                        <div class="dropdown-header h6">HEADING</div>
                        <a class="dropdown-item" href="#">Action</a>
                        <a class="dropdown-item" href="#">Another action</a>
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item" href="#">Something else here</a>
                    </div>
                </li>
                <li class="nav-item">
                    <a class="nav-link disabled" href="#">Disabled</a>
                </li>
            </ul>

            <form class="form-inline my-2 my-lg-0">
                <input class="form-control form-{{ inputPartial }} mr-sm-2" type="search" placeholder="Search" aria-label="Search">
                <button class="btn btn-outline-{{ btnOutlinePartial }} my-2 my-sm-0" type="submit">Search</button>
            </form>

        </div>

    </nav>
</div>
{%- endmacro %}



{%- macro negative_margin_examps(number) %}
<h5 class="mt-4">Negative Margin: (n){{ number }}</h5>
<div class="js-prism pb-2">
    <div class="row mx-md-n{{ number }}">
        <div class="col px-md-{{ number }}">
            <div class="p-2 bg-light">One</div>
        </div>
        <div class="col px-md-{{ number }}">
            <div class="p-2 bg-light">Two</div>
        </div>
    </div>
</div>
{%- endmacro %}



{%- macro opacity_util(number) %}
<div class="js-prism bg-primary text-white p-2 mb-2">
    <div class="opacity-{{ number }}">{{ number }}%</div>
</div>
{%- endmacro %}



{%- macro padding_examps(number) %}
<h5 class="mt-4">Padding: {{ number }}</h5>
