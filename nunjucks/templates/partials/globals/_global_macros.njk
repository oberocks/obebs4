    <button type="button" class="btn btn-{{ classPartial }}" data-obe-color-class="tooltip-{{ classPartial }}" data-toggle="tooltip" data-placement="top" title="{{ namePartial }} Tooltip">{{ namePartial }} Tooltip</button>
</div>
{%- endmacro %}