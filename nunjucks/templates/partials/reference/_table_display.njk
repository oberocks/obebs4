                                    {{ macros.display_responsive_classes('block') }}
                                    {{ macros.display_responsive_classes('table') }}
                                    {{ macros.display_responsive_classes('table-cell') }}
                                    {{ macros.display_responsive_classes('table-row') }}
                                    {{ macros.display_responsive_classes('flex') }}
                                    {{ macros.display_responsive_classes('inline-flex') }}

