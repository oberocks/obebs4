		<div class="container-fluid pt-5 text-center bg-white text-dark">
        
            <div class="container modal-workspace">

                <div class="row justify-content-center">
                    
                    <div id="{{ global_strings['reference']['sections']['3']['id'] }}" class="col-12">
                        
                        {% set headingHeadline = global_strings['reference']['sections']['3']['head'] %}
						{% set headingSubline = global_strings['reference']['sections']['3']['subhead'] %}
						{% set headingDescrip = global_strings['reference']['sections']['3']['descrip'] %}
                        {% include "partials/_columnHeading.njk" %}

                    </div>

                </div>

                <div class="row justify-content-center">

                    <div class="col pb-5">
                        
                        <div class="table-responsive text-left">
                            <table class="js-data-tables-element table table-striped table-hover table-bordered table-sm text-nowrap">
                                
                                {{ macros.responsive_classes_thead('thead-dark') }}
                                
                                <tbody>
                                    
                                    {{ macros.display_responsive_classes('none') }}
                                    {{ macros.display_responsive_classes('inline') }}
