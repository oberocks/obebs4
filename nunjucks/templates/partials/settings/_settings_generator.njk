                        {% macro decoration_select(id, label, selection1, note='false') %}
                        <div class="form-group{% if note != 'false' %} mb-0{% endif %}">
                            <label class="d-block font-weight-bold">{{ label }}</label>
                            <select id="{{ id }}" class="form-control"{% if note != 'false' %} aria-describedby="{{id}}-help"{% endif %}>
                                
                                <option value="none"{% if selection1 == 'none' %} selected{% endif %}>None</option>

                                <option value="underline"{% if selection1 == 'underline' %} selected{% endif %}>Underline</option>
                                <option value="underline dotted"{% if selection1 == 'underline dotted' %} selected{% endif %}>Underline Dotted</option>
                                <option value="underline double"{% if selection1 == 'underline double' %} selected{% endif %}>Underline Double</option>
                                <option value="underline dashed"{% if selection1 == 'underline dashed' %} selected{% endif %}>Underline Dashed</option>
                                <option value="underline wavy"{% if selection1 == 'underline wavy' %} selected{% endif %}>Underline Wavy</option>

                                <option value="underline overline"{% if selection1 == 'underline overline' %} selected{% endif %}>Underline + Overline</option>

                                <option value="overline"{% if selection1 == 'overline' %} selected{% endif %}>Overline</option>
                                <option value="overline dotted"{% if selection1 == 'overline dotted' %} selected{% endif %}>Overline Dotted</option>
                                <option value="overline double"{% if selection1 == 'overline double' %} selected{% endif %}>Overline Double</option>
                                <option value="overline dashed"{% if selection1 == 'overline dashed' %} selected{% endif %}>Overline Dashed</option>
                                <option value="overline wavy"{% if selection1 == 'overline wavy' %} selected{% endif %}>Overline Wavy</option>

